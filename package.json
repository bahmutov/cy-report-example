{
  "name": "cy-report-example",
  "version": "1.0.0",
  "description": "Mochawesome report for Cypress example",
  "scripts": {
    "test": "cypress run",
    "start": "npm start --prefix todomvc",
    "cypress": "cypress open",
    "dev": "start-test 3000 cypress",
    "print-tests": "find-cypress-specs --names --tags",
    "lint": "tsc --noEmit --pretty",
    "ci": "start-test 3000 cy:run",
    "cy:run": "cypress run",
    "postinstall": "npm install --prefix todomvc",
    "report:merge": "mochawesome-merge cypress/results/*.json -o cypress/report.json",
    "report:generate": "marge cypress/report.json --charts true",
    "one:report": "cypress run --spec cypress/e2e/viewports.cy.ts"
  },
  "keywords": [
    "cypress-example"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com> (https://glebbahmutov.com/)",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "cy-spok": "^1.5.2",
    "cypress": "12.4.0",
    "cypress-mochawesome-reporter": "^3.2.3",
    "cypress-split": "^1.1.4",
    "prettier": "2.8.3",
    "start-server-and-test": "1.14.0",
    "typescript": "4.8.3"
  },
  "others": {
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  }
}
